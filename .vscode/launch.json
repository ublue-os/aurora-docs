{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Start Docusaurus Development Server",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "start"]
    },
    {
      "name": "Build Docusaurus Site",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "build"]
    },
    {
      "name": "Serve Production Build",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "serve"]
    }
  ]
}
